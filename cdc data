   "source": [
    "def standard_units(arr):\n",
    "    return (arr - np.average(arr))/np.std(arr)\n",
    "\n",
    "def correlation(tbl, x_col, y_col):\n",
    "    x_standard = standard_units(tbl.column(x_col))\n",
    "    y_standard = standard_units(tbl.column(y_col))\n",
    "    return np.average(x_standard * y_standard)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 72,
   "metadata": {
    "tags": []
   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([ -1.42359142e-02,   1.47976589e+05])"
      ]
     },
     "execution_count": 72,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def fit_line(tbl, x_col, y_col):\n",
    "    r = correlation(tbl, x_col, y_col)\n",
    "    slope = r * np.std(tbl.column(y_col)) / np.std(tbl.column(x_col))\n",
    "    intercept = np.average(tbl.column(y_col) - slope * np.average(tbl.column(x_col)))\n",
    "    return make_array(slope, intercept)\n",
    "\n",
    "# Now to predict Flights based on PIC Deaths\n",
    "fit_line(Health, \"Flights\", \"PIC Death\")"
   ]
  },
  {
   "cell_type": "code",
