   "source": [
    "slope, intercept = fit_line(Health, \"PIC Death\", \"Flights\")\n",
    "Health.scatter(\"PIC Death\", \"Flights\")\n",
    "plt.plot([min(Health.column(\"PIC Death\")), max(Health.column(\"PIC Death\"))], \n",
    "         [slope*min(Health.column(\"PIC Death\"))+intercept, slope*max(Health.column(\"PIC Death\"))+intercept])\n",
    "plt.show()"
